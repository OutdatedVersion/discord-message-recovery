{
    "compilerOptions": {
        "lib": ["es5", "es6"],
        "target": "es6",
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "./build",
        "importHelpers": true,
        "strict": true,
        "strictNullChecks": false,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "node"
    }
}
